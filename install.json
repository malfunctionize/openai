{
  "run": [{
    "method": "input",
    "params": {
      "title": "API Key",
      "form": [{
        "key": "key",
        "description": "Enter your OpenAI API Key"
      }]
    }
  }, {
    "method": "set",
    "params": {
      "self": {
        "config.json": {
          "key": "{{input.key}}"
        }
      }
    }
  }]
}
